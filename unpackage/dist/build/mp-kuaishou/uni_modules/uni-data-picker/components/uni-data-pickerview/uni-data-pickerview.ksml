<view class="uni-data-pickerview"><block ks:if="{{!isCloudDataList}}"><scroll-view class="selected-area" scroll-x="true"><view class="selected-list"><block ks:for="{{selected}}" ks:for-item="item" ks:for-index="index" ks:key="index"><view data-event-opts="{{[['tap',[['handleSelect',[index]]]]]}}" class="{{['selected-item',(index==selectedIndex)?'selected-item-active':'']}}" bindtap="__e"><text>{{item.text||''}}</text></view></block></view></scroll-view></block><view class="tab-c"><scroll-view class="list" scroll-y="{{true}}"><block ks:for="{{dataList[selectedIndex]}}" ks:for-item="item" ks:for-index="j" ks:key="j"><view data-event-opts="{{[['tap',[['handleNodeClick',['$0','$1',j],[[['dataList.'+selectedIndex+'','',j]],'selectedIndex']]]]]}}" class="{{['item',(!!item.disable)?'is-disabled':'']}}" bindtap="__e"><text class="item-text">{{item[map.text]}}</text><block ks:if="{{selected.length>selectedIndex&&item[map.value]==selected[selectedIndex].value}}"><view class="check"></view></block></view></block></scroll-view><block ks:if="{{loading}}"><view class="loading-cover"><uni-load-more class="load-more" vue-id="d6f27e3a-1" contentText="{{loadMore}}" status="loading" bind:__l="__l"></uni-load-more></view></block><block ks:if="{{errorMessage}}"><view class="error-message"><text class="error-text">{{errorMessage}}</text></view></block></view></view>