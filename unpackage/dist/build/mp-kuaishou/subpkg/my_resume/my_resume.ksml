<view class="content"><custom-header vue-id="4c499251-1" title="{{pageName}}" showBack="{{true}}" headHeight="{{headerHeight}}" bind:__l="__l"></custom-header><view class="box" style="{{'top:'+(marginTop+tabMargin+'px')+';'}}"><scroll-view style="{{'height:'+(scrollHeight+'px')+';'}}" scroll-y="true"><view class="head flex flex_btween"><view class="avatar flex flex-start"><image src="{{baseInfo.avatar}}" mode="widthFix"></image><view>{{baseInfo.name}}</view></view><view class="finish"><view>简历完成度</view><view class="fini_status">{{mission}}</view></view></view><view class="body" style="{{'margin-top:'+(tabMargin+'px')+';'+('padding-bottom:'+(btnHeight+3*tabMargin+'px')+';')}}"><view class="body_box"><view class="title">基本信息</view><view class="info" style="margin-top:0;"><view class="line flex flex-start"><view class="tit">性别：</view><view class="text">{{baseInfo.gender=="female"?"女":"男"}}</view></view><view class="line flex flex-start"><view class="tit">年龄：</view><view class="text">{{baseInfo.age}}</view></view><view class="line flex flex-start"><view class="tit">手机号：</view><view class="text">{{baseInfo.mobile}}</view></view></view></view><view class="body_box"><view class="title">工作意向</view><view class="info"><view class="input_line"><view class="tit">意向城市</view><view data-event-opts="{{[['tap',[['openCity',['$event']]]]]}}" class="input_wrap flex flex_btween" bindtap="__e"><view class="city" style="width:calc(100% - 13rpx);">{{''+(wannerCity?wannerCity:"请选择意向城市")+''}}</view><image class="arrow" src="{{imgUrl+'/worker/ic_arrow_right_yellow.png'}}" mode="widthFix"></image></view></view><view class="input_line"><view class="tit">意向工种</view><view class="labels flex flex_btween"><block ks:for="{{workType}}" ks:for-item="item" ks:for-index="index" ks:key="index"><view data-event-opts="{{[['tap',[['chooseWorkType',[index]]]]]}}" class="{{['label',item.active?'active':'']}}" bindtap="__e">{{''+item.job_type_name+''}}</view></block></view></view></view></view><block ks:if="{{seleType.length}}"><view class="body_box"><view class="title">工作偏好</view><view class="info"><block ks:for="{{$root.l0}}" ks:for-item="item" ks:for-index="index" ks:key="index"><view class="input_line"><view class="subtit">{{item.$orig.job_type_name}}</view><view class="input_line"><view class="tit">期望薪资</view><view class="input_wrap salary flex flex_btween"><input type="number" placeholder="最低薪资" data-event-opts="{{[['input',[['__set_model',['$0','desired_salary_min','$event',[]],[[['seleType','',index]]]]]]]}}" value="{{item.$orig.desired_salary_min}}" bindinput="__e"/><view>~</view><input type="number" placeholder="最高薪资" data-event-opts="{{[['input',[['__set_model',['$0','desired_salary_max','$event',[]],[[['seleType','',index]]]]]]]}}" value="{{item.$orig.desired_salary_max}}" bindinput="__e"/><view class="period">元</view></view></view><view class="input_line"><view class="tit">结算方式</view><view class="input_wrap flex flex_around"><block ks:for="{{settleTye}}" ks:for-item="item2" ks:for-index="index2" ks:key="index2"><view data-event-opts="{{[['tap',[['chooseSettleType',['$0',index],[[['settleTye','',index2]]]]]]]}}" class="radio_item flex flex-start" bindtap="__e"><image src="{{item.$orig.salary_pay_period==item2.value?imgUrl+'/worker/ic_circle_fill.png':imgUrl+'/worker/ic_circle_void.png'}}" mode></image><view>{{item2.text}}</view></view></block></view></view><view class="input_line"><view class="tit">我的优势</view><view data-event-opts="{{[['tap',[['openTag',['$0',index],[[['seleType','',index]]]]]]]}}" class="input_wrap adv" style="{{'color:'+(item.$orig.advantages.length?'#333':'#9E9E9E')+';'}}" bindtap="__e">{{''+(!item.$orig.advantages.length?"请选择工作优势":item.m0)+''}}<image src="{{imgUrl+'/worker/ic_arrow_right_yellow.png'}}" mode="widthFix"></image></view></view></view></block></view></view></block><view class="body_box"><view class="title">自我介绍</view><view class="info"><view data-event-opts="{{[['tap',[['toVoice',['$event']]]]]}}" class="input_wrap intro" bin