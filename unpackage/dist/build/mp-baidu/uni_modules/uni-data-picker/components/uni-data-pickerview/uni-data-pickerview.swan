<view class="uni-data-pickerview"><block s-if="{{!isCloudDataList}}"><scroll-view class="selected-area" scroll-x="true"><view class="selected-list"><block s-for="selected trackBy index" s-for-item="item" s-for-index="index"><view data-event-opts="{{[['tap',[['handleSelect',[index]]]]]}}" class="{{['selected-item',(index==selectedIndex)?'selected-item-active':'']}}" bindtap="__e"><text>{{item.text||''}}</text></view></block></view></scroll-view></block><view class="tab-c"><scroll-view class="list" scroll-y="{{true}}"><block s-for="dataList[selectedIndex] trackBy j" s-for-item="item" s-for-index="j"><view data-event-opts="{{[['tap',[['handleNodeClick',['$0','$1',j],[[['dataList.'+selectedIndex+'','',j]],'selectedIndex']]]]]}}" class="{{['item',(!!item.disable)?'is-disabled':'']}}" bindtap="__e"><text class="item-text">{{item[map.text]}}</text><block s-if="{{selected.length>selectedIndex&&item[map.value]==selected[selectedIndex].value}}"><view class="check"></view></block></view></block></scroll-view><block s-if="{{loading}}"><view class="loading-cover"><uni-load-more class="load-more" vue-id="d6eb1d26-1" contentText="{{loadMore}}" status="loading"></uni-load-more></view></block><block s-if="{{errorMessage}}"><view class="error-message"><text class="error-text">{{errorMessage}}</text></view></block></view></view>