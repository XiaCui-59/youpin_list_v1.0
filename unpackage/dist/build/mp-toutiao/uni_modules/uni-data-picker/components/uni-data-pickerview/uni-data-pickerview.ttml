<view class="uni-data-pickerview"><block tt:if="{{!isCloudDataList}}"><scroll-view class="selected-area" scroll-x="true"><view class="selected-list"><block tt:for="{{selected}}" tt:for-item="item" tt:for-index="index" tt:key="index"><view data-event-opts="{{[['tap',[['handleSelect',[index]]]]]}}" class="{{(('selected-item')+' '+((index==selectedIndex)?'selected-item-active':''))}}" bindtap="__e"><text>{{item.text||''}}</text></view></block></view></scroll-view></block><view class="tab-c"><scroll-view class="list" scroll-y="{{true}}"><block tt:for="{{dataList[selectedIndex]}}" tt:for-item="item" tt:for-index="j" tt:key="j"><view data-event-opts="{{[['tap',[['handleNodeClick',['$0','$1',j],[[['dataList.'+selectedIndex+'','',j]],'selectedIndex']]]]]}}" class="{{(('item')+' '+((!!item.disable)?'is-disabled':''))}}" bindtap="__e"><text class="item-text">{{item[map.text]}}</text><block tt:if="{{selected.length>selectedIndex&&item[map.value]==selected[selectedIndex].value}}"><view class="check"></view></block></view></block></scroll-view><block tt:if="{{loading}}"><view class="loading-cover"><uni-load-more class="load-more" vue-id="d6ea480a-1" contentText="{{loadMore}}" status="loading" bind:__l="__l"></uni-load-more></view></block><block tt:if="{{errorMessage}}"><view class="error-message"><text class="error-text">{{errorMessage}}</text></view></block></view></view>