image{will-change:transform;width:auto;height:auto}.tags .tags_box{width:100%}.tags .tags_box .title{font-weight:600;position:relative;margin-bottom:50rpx}.tags .tags_box .title text{font-weight:400;font-size:27rpx;color:#a8a8a8}.tags .tags_box .btns{margin-top:80rpx}.tags .tags_box .btns .btn{width:48%;height:84rpx;line-height:84rpx;text-align:center;border:1px solid #216ff9;border-radius:16rpx}.tags .tags_box .btns .btn.cancel{color:#216ff9}.tags .tags_box .btns .btn.sure{background:#216ff9;color:#fff}.tags .tags_box .item_wrap{font-size:14px;flex-wrap:wrap}.tags .tags_box .item_wrap .tag{height:76rpx;line-height:76rpx;width:calc(32% - 6rpx);border-radius:6rpx;background:#f9f9f9;border:2rpx solid #f9f9f9;margin-right:2%;margin-top:28rpx;cursor:pointer;position:relative;font-size:23rpx;color:#333;text-align:center}.tags .tags_box .item_wrap .tag:nth-child(3n){margin-right:0}.tags .tags_box .item_wrap .tag image{position:absolute;top:0;right:0;width:38rpx;height:38rpx}.tags .tags_box .item_wrap .tag.active{font-weight:600;border:2rpx solid #216ff9;color:#216ff9}