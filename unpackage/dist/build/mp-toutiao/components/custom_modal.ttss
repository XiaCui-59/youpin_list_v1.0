image{will-change:transform;width:auto;height:auto}.modal{position:fixed;top:0;left:0;right:0;bottom:0;background:rgba(0,0,0,.5);z-index:100}.modal .mod_inner_box{width:71.33%;position:absolute;top:50%;left:50%;-webkit-transform:translate(-50%,-50%);transform:translate(-50%,-50%)}.modal .mod_inner_box .bg{width:100%;height:auto}.modal .mod_inner_box .icon{position:absolute;width:30%;top:20rpx}.modal .mod_inner_box .icon.left{left:70rpx}.modal .mod_inner_box .icon.right{right:70rpx}.modal .mod_inner_box .info{width:100%;position:absolute;top:50%;left:50%;-webkit-transform:translate(-50%,-50%);transform:translate(-50%,-50%);text-align:center}.modal .mod_inner_box .info image{width:60%;margin-bottom:14rpx}.modal .mod_inner_box .info view{font-weight:600;font-size:35rpx;color:#ffb103;text-align:center;padding:0 20rpx;box-sizing:border-box}.modal .mod_inner_box .info .info_tit{margin-bottom:20rpx}.modal .mod_inner_box .info .info_cont{font-weight:500;font-size:32rpx}.modal .mod_inner_box .btns{width:100%;position:absolute;bottom:10%;left:0;padding:0 50rpx;box-sizing:border-box}.modal .mod_inner_box .btns .mod_btn{width:45%;height:85rpx;line-height:85rpx;border-radius:15rpx;text-align:center;font-weight:600;font-size:29rpx;box-shadow:0rpx 2rpx 4rpx 0rpx rgba(0,0,0,.15)}