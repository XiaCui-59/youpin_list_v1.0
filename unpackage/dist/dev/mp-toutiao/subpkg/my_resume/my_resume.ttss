::-webkit-input-placeholder {
  font-weight: 400;
  font-size: 27rpx;
  color: #9E9E9E;
}
 .input-value-border, .input-value {
  border: none !important;
  height: 84rpx !important;
  font-size: 28rpx !important;
}
.fixed_btn {
  position: fixed;
  width: 100%;
  bottom: 0;
  left: 0;
  z-index: 11;
  background: #fff;
  padding: 0 38rpx;
  box-sizing: border-box;
  box-shadow: 0rpx 0rpx 4rpx 0rpx rgba(0, 0, 0, 0.1);
}
.fixed_btn .btn {
  width: 100%;
  background: #216FF9;
  border-radius: 15rpx;
  border: 2rpx solid #216FF9;
  font-weight: 600;
  font-size: 29rpx;
  color: #FFFFFF;
  text-align: center;
}
.mask {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.6);
  z-index: 15;
}
.mask .inner {
  width: 80%;
  height: 100%;
  background: #fff;
  position: absolute;
  top: 0;
  right: 0;
  box-sizing: border-box;
  padding-left: 40rpx;
  padding-right: 40rpx;
}
.mask .inner .title {
  font-size: 30rpx;
  font-weight: 600;
  margin-bottom: 40rpx;
}
.mask .inner .title text {
  color: #f00;
  font-size: 24rpx;
  font-weight: 500;
}
.mask .inner .line {
  margin-bottom: 30rpx;
}
.mask .inner .line .tit {
  width: 120rpx;
  font-size: 28rpx;
  color: #5a5a5a;
}
.mask .inner .line .wrap {
  width: calc(100% - 120rpx);
  height: 84rpx;
  background: #F9F9F9;
  border-radius: 8rpx;
}
.mask .inner .btns {
  margin-top: 80rpx;
}
.mask .inner .btns .btn {
  width: 48%;
  height: 84rpx;
  line-height: 84rpx;
  text-align: center;
  border: 1px solid #216FF9;
  border-radius: 16rpx;
}
.mask .inner .btns .btn.cancel {
  color: #216FF9;
}
.mask .inner .btns .btn.sure {
  background: #216FF9;
  color: #fff;
}
.box {
  position: fixed;
  width: calc(100% - 92rpx);
  left: 46rpx;
  z-index: 10;
}
.box .head {
  padding: 20rpx;
  box-sizing: border-box;
  background: url("https://static.test.swiftwd.com/worker/top_header_bg.png") no-repeat;
  background-size: 100% 100%;
  border-radius: 16rpx;
}
.box .head .avatar image {
  width: 139rpx;
  height: 139rpx;
  margin-right: 28rpx;
  border-radius: 50%;
}
.box .head .avatar view {
  font-weight: 600;
  font-size: 33rpx;
  color: #FFFFFF;
  text-shadow: 0px 2px 2px rgba(0, 0, 0, 0.2);
}
.box .head .finish {
  font-weight: 400;
  font-size: 23rpx;
  color: #FFFFFF;
  text-align: center;
}
.box .head .finish .fini_status {
  width: 135rpx;
  height: 44rpx;
  line-height: 44rpx;
  background: rgba(255, 255, 255, 0.2);
  border-radius: 4rpx;
  font-family: Arial, Arial;
  font-weight: 900;
  font-size: 35rpx;
  color: #FFFFFF;
  margin-top: 10rpx;
}
.box .body {
  background: #fff;
  border-radius: 16rpx;
  padding: 40rpx 50rpx;
  box-sizing: border-box;
}
.box .body .body_box {
  margin-bottom: 58rpx;
}
.box .body .body_box:last-child {
  margin-bottom: 0;
}
.box .body .body_box .title {
  font-size: 29rpx;
  color: #333333;
  position: relative;
  padding-left: 16rpx;
  box-sizing: border-box;
}
.box .body .body_box .title::before {
  content: "";
  position: absolute;
  width: 8rpx;
  height: 27rpx;
  background: #216FF9;
  border-radius: 2rpx;
  left: 0;
  top: 50%;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
}
.box .body .body_box .info {
  margin-top: 20rpx;
}
.box .body .body_box .info .tit {
  font-size: 27rpx;
  color: #5A5A5A;
}
.box .body .body_box .info .line {
  height: 84rpx;
  line-height: 84rpx;
  font-weight: 400;
  border-bottom: 2rpx dashed #DFDFDF;
}
.box .body .body_box .info .line:last-child {
  border-bottom: none;
}
.box .body .body_box .info .line .text {
  font-size: 25rpx;
  color: #333333;
}
.box .body .body_box .info > .input_wrap {
  height: 84rpx;
  background: #F9F9F9;
  border-radius: 8rpx;
  padding: 0 40rpx;
  box-sizing: border-box;
  margin-top: 8rpx;
}
.box .body .body_box .info > .input_wrap.intro {
  position: relative;
  padding: 20rpx 70rpx 20rpx 40rpx;
  height: auto;
  line-height: 40rpx;
  font-weight: 400;
  font-size: 27rpx;
  color: #5A5A5A;
}
.box .body .body_box .info > .input_wrap.intro image {
  width: 30rpx;
  height: 42rpx;
  position: absolute;
  top: 20rpx;
  right: 40rpx;
}
.box .body .body_box .info .input_line {
  margin-bottom: 38rpx;
}
.box .body .body_box .info .input_line .subtit {
  height: 58rpx;
  line-height: 58rpx;
  text-align: center;
  background: rgba(33, 111, 249, 0.2);
  border-radius: 2rpx;
  font-weight: 600;
  font-size: 27rpx;
  color: #216FF9;
  margin-bottom: 19rpx;
}
.box .body .body_box .info .input_line .labels {
  margin-top: 8rpx;
}
.box .body .body_box .info .input_line .labels .label {
  width: 30%;
  height: 85rpx;
  line-height: 85rpx;
  text-align: center;
  background: #F9F9F9;
  border-radius: 6rpx;
  font-weight: 400;
  font-size: 27rpx;
  color: #333333;
  border: 2rpx solid #F9F9F9;
}
.box .body .body_box .info .input_line .labels .label.active {
  border: 2rpx solid #216FF9;
  font-weight: 600;
  font-size: 27rpx;
  color: #216FF9;
  position: relative;
}
.box .body .body_box .info .input_line .labels .label.active::after {
  content: "";
  width: 38rpx;
  height: 38rpx;
  position: absolute;
  top: 0;
  right: 0;
  background: url("https://static.test.swiftwd.com/worker/ic_work_sele_1.png") no-repeat;
  background-size: 100% 100%;
}
.box .body .body_box .info .input_line .input_wrap {
  height: 84rpx;
  background: #F9F9F9;
  border-radius: 8rpx;
  padding: 0 40rpx;
  box-sizing: border-box;
  margin-top: 8rpx;
}
.box .body .body_box .info .input_line .input_wrap .arrow {
  width: 13rpx;
  height: 23rpx;
}
.box .body .body_box .info .input_line .input_wrap.adv {
  height: auto;
  padding: 20rpx 55rpx 20rpx 40rpx;
  line-height: 40rpx;
  font-weight: 400;
  font-size: 27rpx;
  color: #9E9E9E;
  position: relative;
}
.box .body .body_box .info .input_line .input_wrap.adv image {
  width: 13rpx;
  height: 23rpx;
  position: absolute;
  top: 50%;
  right: 40rpx;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
}
.box .body .body_box .info .input_line .input_wrap.salary .period {
  font-weight: 400;
  font-size: 29rpx;
  color: #A8A8A8;
}
.box .body .body_box .info .input_line .input_wrap.salary input {
  text-align: center;
}
.box .body .body_box .info .input_line .input_wrap .city {
  font-size: 27rpx;
  color: #333333;
}
.box .body .body_box .info .input_line .input_wrap input {
  height: 84rpx;
  font-weight: 400;
  font-size: 27rpx;
  color: #333333;
}
.box .body .body_box .info .input_line .input_wrap .radio_item {
  font-size: 27rpx;
  color: #333333;
}
.box .body .body_box .info .input_line .input_wrap .radio_item image {
  width: 38rpx;
  height: 38rpx;
  margin-right: 8rpx;
}

