<view class="content data-v-4fe5b58c"><custom-header vue-id="29927bd3-1" title="{{pageName}}" showBack="{{true}}" headHeight="{{headerHeight}}" class="data-v-4fe5b58c" bind:__l="__l"></custom-header><view class="cont data-v-4fe5b58c" style="{{'margin-top:'+(marginTop+'px')+';'}}"><view class="box data-v-4fe5b58c" style="{{'top:'+(marginTop+'px')+';'}}"><view class="wrap data-v-4fe5b58c"><block ks:if="{{hasOldPassword}}"><view class="line data-v-4fe5b58c"><view class="tit data-v-4fe5b58c">旧密码</view><view class="input_wrap flex flex_btween data-v-4fe5b58c"><input type="password" placeholder="请输入旧密码" placeholder-style="font-size:28rpx;" data-event-opts="{{[['input',[['__set_model',['','oldPassword','$event',[]]],['checkActive',['$event']]]]]}}" value="{{oldPassword}}" bindinput="__e" class="data-v-4fe5b58c"/><image src="{{imgUrl+'/worker/ic_close.png'}}" mode="widthFix" data-event-opts="{{[['tap',[['reset',['old']]]]]}}" bindtap="__e" class="data-v-4fe5b58c"></image></view></view></block><view class="line data-v-4fe5b58c"><view class="tit data-v-4fe5b58c">新密码</view><view class="input_wrap flex flex_btween data-v-4fe5b58c"><input type="password" placeholder="请输入新密码" placeholder-style="font-size:28rpx;" data-event-opts="{{[['input',[['__set_model',['','newPassword','$event',[]]],['checkActive',['$event']]]]]}}" value="{{newPassword}}" bindinput="__e" class="data-v-4fe5b58c"/><image src="{{imgUrl+'/worker/ic_close.png'}}" mode="widthFix" data-event-opts="{{[['tap',[['reset',['new']]]]]}}" bindtap="__e" class="data-v-4fe5b58c"></image></view></view><view class="line data-v-4fe5b58c"><view class="tit data-v-4fe5b58c">确认新密码</view><view class="input_wrap flex flex_btween data-v-4fe5b58c"><input type="password" placeholder="请再次输入新密码" placeholder-style="font-size:28rpx;" data-event-opts="{{[['input',[['__set_model',['','veriPassword','$event',[]]],['checkActive',['$event']]]]]}}" value="{{veriPassword}}" bindinput="__e" class="data-v-4fe5b58c"/><image src="{{imgUrl+'/worker/ic_