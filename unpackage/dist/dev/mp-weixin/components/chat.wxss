.voice_icon {
  height: 34rpx;
  width: 34rpx;
  background-repeat: no-repeat;
  background-size: 100%;
  flex-shrink: 0;
}
.voice_icon_right {
  background-image: url("https://static.test.swiftwd.com/worker/voice-left-3.png");
  margin-left: 10rpx;
}
.voice_icon_right_an {
  -webkit-animation: voiceAn_right 1s linear alternate infinite;
          animation: voiceAn_right 1s linear alternate infinite;
}
@-webkit-keyframes voiceAn_right {
0% {
    background-image: url("https://static.test.swiftwd.com/worker/voice-left-1.png");
}
50% {
    background-image: url("https://static.test.swiftwd.com/worker/voice-left-2.png");
}
100% {
    background-image: url("https://static.test.swiftwd.com/worker/voice-left-3.png");
}
}
@keyframes voiceAn_right {
0% {
    background-image: url("https://static.test.swiftwd.com/worker/voice-left-1.png");
}
50% {
    background-image: url("https://static.test.swiftwd.com/worker/voice-left-2.png");
}
100% {
    background-image: url("https://static.test.swiftwd.com/worker/voice-left-3.png");
}
}
