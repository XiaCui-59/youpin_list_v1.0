/* 文章场景相关 */
.report {
  height: 100vh;
  background: linear-gradient(180deg, #4290FF 0%, #91BAFF 9%, #F0F5FE 60%);
  color: #010101;
}
.report .cont {
  position: fixed;
  left: 0;
  width: 100%;
  overflow-y: scroll;
}
.report .cont .nodata {
  position: absolute;
  top: 40%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
  text-align: center;
}
.report .cont .nodata .img image {
  width: 236rpx;
}
.report .cont .nodata .text {
  font-weight: 400;
  font-size: 29rpx;
  color: #333333;
  line-height: 40rpx;
}
.report .cont .nodata .btn {
  margin: 17rpx auto 0 auto;
  width: 212rpx;
  height: 77rpx;
  line-height: 77rpx;
  background: #216FF9;
  box-shadow: 0rpx 2rpx 4rpx 0rpx rgba(0, 0, 0, 0.15);
  border-radius: 15rpx;
  font-weight: 400;
  font-size: 29rpx;
  color: #FFFFFF;
}
.report .cont .base_info {
  background: #1677FF;
  padding: 21rpx 0rpx 21rpx 28rpx;
}
.report .cont .base_info .info {
  margin-left: 19rpx;
}
.report .cont .base_info .info .name {
  font-weight: 600;
  font-size: 35rpx;
  color: #FFFFFF;
  line-height: 48rpx;
}
.report .cont .base_info .info .text {
  font-weight: 400;
  font-size: 23rpx;
  color: #D8D8D8;
  line-height: 40rpx;
}
.report .cont .more {
  padding: 38rpx 28rpx;
  background: #FFFFFF;
}
.report .cont .more .box {
  margin-top: 50rpx;
}
.report .cont .more .box .title {
  font-weight: 400;
  font-size: 27rpx;
  color: #333333;
  position: relative;
  padding: 19rpx 0 19rpx 19rpx;
  border-bottom: 2rpx dotted #C3D9FF;
}
.report .cont .more .box .title::before {
  position: absolute;
  top: 50%;
  left: 0;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
  content: "";
  width: 8rpx;
  height: 27rpx;
  background: #1677FF;
  border-radius: 4rpx;
}
.report .cont .more .box .box_cont .item {
  margin: 19rpx 0;
  background: #F3F5FF;
  border-radius: 6rpx;
  padding: 28rpx 19rpx 19rpx 19rpx;
}
.report .cont .more .box .box_cont .item.item2 {
  padding: 28rpx 19rpx 19rpx 23rpx;
}
.report .cont .more .box .box_cont .item .left {
  width: 67rpx;
  height: 67rpx;
  border-radius: 50%;
  background: #D5E4FE;
  color: #1677FF;
  font-weight: 600;
  font-size: 34rpx;
  text-align: center;
  line-height: 67rpx;
  margin-right: 17rpx;
  flex-shrink: 0;
}
.report .cont .more .box .box_cont .item .right .subtit {
  font-weight: 600;
  font-size: 27rpx;
  color: #333333;
  line-height: 38rpx;
  margin-bottom: 8rpx;
}
.report .cont .more .box .box_cont .item .right .text {
  font-weight: 400;
  font-size: 23rpx;
  color: #333333;
  line-height: 33rpx;
}
.report .cont .more .box .box_cont .item .top {
  margin-bottom: 15rpx;
}
.report .cont .more .box .box_cont .item .top .subtit {
  font-weight: 600;
  font-size: 27rpx;
  color: #333333;
  flex-shrink: 0;
  margin-right: 38rpx;
}
.report .cont .more .box .box_cont .item .top .right {
  width: 100%;
}
.report .cont .more .box .box_cont .item .top .right .percent {
  font-weight: 600;
  font-size: 27rpx;
  color: #1677FF;
  margin-left: 10rpx;
}
.report .cont .more .box .box_cont .item .bot {
  font-weight: 400;
  font-size: 23rpx;
  color: #6B6B6B;
  line-height: 33rpx;
}
.report .cont .more .more_base .name {
  font-weight: 600;
  font-size: 35rpx;
  color: #333333;
}
.report .cont .more .more_base .status {
  font-weight: 600;
  font-size: 31rpx;
  color: #FAAD14;
}
.report .cont .more .more_base .summary {
  width: calc(100% - 200rpx);
  flex-shrink: 0;
  font-weight: 400;
  font-size: 27rpx;
  color: #333333;
  line-height: 42rpx;
}
.report .cont .more .more_base .charts-box {
  width: 150rpx;
  height: 150rpx;
}
.report .cont .more .more_base .labels {
  flex-wrap: wrap;
}
.report .cont .more .more_base .labels .label {
  padding: 10rpx 17rpx;
  background: #F3F5FF;
  border-radius: 25rpx;
  font-weight: 400;
  font-size: 23rpx;
  color: #1677FF;
  margin-right: 15rpx;
  margin-bottom: 8rpx;
}
.report .cont .more .more_base .labels .label.warning {
  background: #FEF3C6;
  color: #D68629;
}
.report .cont .more .more_base .next {
  font-weight: 400;
  font-size: 27rpx;
  color: #333;
  line-height: 46rpx;
  background: #F3F5FF;
  border-radius: 8rpx;
  padding: 11rpx 19rpx;
  margin-top: 10rpx;
  margin-bottom: 19rpx;
}
.report .cont .more .more_base .next text {
  color: #0045A6;
}
.report .cont .more .more_base .btns .btn {
  width: 212rpx;
  height: 77rpx;
  background: rgba(33, 111, 249, 0.19);
  box-shadow: 0rpx 2rpx 4rpx 0rpx rgba(0, 0, 0, 0.15);
  border-radius: 15rpx;
  border: 2rpx solid #216FF9;
  font-weight: 400;
  font-size: 29rpx;
  color: #1677FF;
  line-height: 77rpx;
  text-align: center;
}
.report .cont .more .more_base .btns .btn:nth-child(2) {
  background: #216FF9;
  color: #FFFFFF;
}
