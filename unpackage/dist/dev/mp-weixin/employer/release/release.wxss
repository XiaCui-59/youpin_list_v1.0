.kefu {
  position: fixed;
  right: 0;
  width: 161rpx;
  height: 120rpx;
  background: #FFFFFF;
  box-shadow: 0rpx 2rpx 6rpx 0rpx rgba(0, 0, 0, 0.2);
  border-top-left-radius: 56rpx;
  border-bottom-left-radius: 56rpx;
  z-index: 20;
}
.kefu .kefu_in {
  position: absolute;
  top: 50%;
  left: 53%;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
  text-align: center;
}
.kefu .kefu_in image {
  width: 65rpx;
}
.kefu .kefu_in view {
  font-weight: 400;
  font-size: 23rpx;
  color: #333333;
  white-space: nowrap;
}
 .u-calendar-month__days__day__select__buttom-info {
  bottom: -10px !important;
}
 .input-value, .uni-data-tree, .uni-data-tree > view {
  height: 100% !important;
  padding: 0 !important;
}
 .input-value-border {
  border: none !important;
}
 .input-arrow {
  border-left: 1px solid #F7BC05 !important;
  border-bottom: 1px solid #F7BC05 !important;
}
 .u-radio-group {
  flex: 0 !important;
}
 .selected-item-active {
  border-bottom: 2px solid #F7BC05 !important;
}
 .check {
  border-color: #F7BC05 !important;
}
.inner {
  width: calc(100% - 92rpx) !important;
  color: #333;
  padding: 30rpx 38rpx 76rpx 38rpx !important;
  background: linear-gradient(180deg, #FCD1A8 0%, #FFF9F0 20%) !important;
  border-radius: 37rpx;
}
.inner .bg_ic {
  font-size: 0;
  position: absolute;
  top: 0;
  right: 0;
  z-index: 99;
}
.inner .bg_ic image {
  width: 250rpx;
}
.inner .title {
  position: relative;
  z-index: 1000;
  font-weight: 600;
  font-size: 38rpx;
  color: #282319;
  line-height: 54rpx;
  margin-bottom: 55rpx;
  padding-left: 38rpx;
}
.inner .box {
  position: relative;
  z-index: 1000;
}
.inner .box .line {
  height: 85rpx;
  line-height: 85rpx;
  margin: 20rpx 0;
  font-size: 29rpx;
  background: #FFFFFF;
  box-shadow: 0rpx 2rpx 6rpx 0rpx rgba(0, 0, 0, 0.1);
  border-radius: 8rpx;
  padding: 0 38rpx 0 23rpx;
}
.inner .box .line .line_tit {
  width: 50%;
}
.inner .box .line .line_tit image {
  width: 38rpx;
  margin-right: 13rpx;
}
.inner .box .line .line_tit text {
  font-weight: 400;
  font-size: 27rpx;
  color: #5A5A5A;
}
.inner .box .line .line_text {
  width: 50%;
  color: #666;
}
.inner .box .line .line_text view:nth-child(1) {
  font-weight: 600;
  font-size: 27rpx;
  color: #333333;
}
.inner .box .line .line_text view:nth-child(2) {
  font-weight: 400;
  font-size: 23rpx;
  color: #A8A8A8;
}
.inner .box .tips {
  font-size: 24rpx;
}
.inner .box .tips .tips_item {
  margin-top: 10rpx;
  align-items: flex-start;
}
.inner .box .tips .tips_item .tips_tit {
  white-space: nowrap;
}
.inner .box .tips .tips_item .tips_text {
  font-size: 23rpx;
  color: #DD0000;
}
.inner .box .btn {
  position: relative;
  top: 0;
  bottom: 0;
  left: 0;
  margin-top: 38rpx;
  height: 94rpx;
  width: 100%;
  flex-direction: column;
  justify-content: space-around;
}
.inner .box .btn > view {
  flex-direction: column;
}
.inner .box .btn .tips {
  font-weight: 400;
  font-size: 23rpx;
}
.release {
  height: 100vh;
  background: #F7F8FA;
  color: #010101;
}
.release .btn {
  position: fixed;
  width: calc(100% - 92rpx);
  left: 46rpx;
}
.release .btn .btn_y,
.release .btn .btn_w {
  width: 48%;
}
.release .cont {
  position: fixed;
  left: 0;
  width: 100%;
  overflow-y: scroll;
}
.release .cont .cont_mid {
  padding: 0 46rpx;
}
.release .cont .cont_mid .box .box_in {
  box-sizing: border-box;
  background: #FFFFFF;
  box-shadow: 0px 1px 3px 0px rgba(0, 0, 0, 0.15);
  border-radius: 15rpx;
}
.release .cont .cont_mid .box .box_in .in_item {
  position: relative;
}
.release .cont .cont_mid .box .box_in .in_item .in_item_tips {
  position: absolute;
  bottom: 0;
  right: 0;
  color: #DB0000;
  font-size: 24rpx;
}
.release .cont .cont_mid .box .box_in .in_item .in_tit {
  font-weight: 400;
  font-size: 27rpx;
  color: #5A5A5A;
}
.release .cont .cont_mid .box .box_in .in_item .in_tit text {
  font-weight: 600;
  color: #DB0000;
}
.release .cont .cont_mid .box .box_in .in_item .input_wrap {
  background: #F9F9F9;
  border-radius: 8rpx;
  padding: 0 38rpx 0 19rpx;
  font-size: 12px;
  position: relative;
  white-space: nowrap;
  overflow: scroll;
}
.release .cont .cont_mid .box .box_in .in_item .input_wrap .clear {
  position: absolute;
  top: 50%;
  right: 0rpx;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
  padding: 30rpx;
  z-index: 99;
}
.release .cont .cont_mid .box .box_in .in_item .input_wrap .clear image {
  width: 38rpx;
}
.release .cont .cont_mid .box .box_in .in_item .input_wrap textarea {
  padding: 23rpx 26rpx 38rpx 19rpx;
  box-sizing: border-box;
  font-weight: 400;
  color: #333;
  line-height: 38rpx;
}
.release .cont .cont_mid .box .box_in .in_item .input_wrap .line {
  width: 45%;
  text-align: center;
}
.release .cont .cont_mid .box .box_in .in_item .input_wrap .line.mark {
  width: 10%;
  color: #777;
}
.release .cont .cont_head {
  background: #FFFFFF;
}
.release .cont .cont_head .step.active .num {
  background: #F7BC05;
  color: #fff;
}
.release .cont .cont_head .step.active .text {
  font-weight: 600;
  color: #F7BC05;
}
.release .cont .cont_head .step .num {
  width: 50rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  background: rgba(247, 188, 5, 0.1);
  font-weight: 600;
  font-size: 31rpx;
  color: #F7BC05;
  border-radius: 50%;
  margin-right: 10rpx;
}
.release .cont .cont_head .step .text {
  font-weight: 400;
  font-size: 29rpx;
  color: #333333;
}
.release .cont .cont_head image {
  width: 38rpx;
  margin: 0 67rpx;
}
